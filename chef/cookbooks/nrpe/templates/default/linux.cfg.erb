# Generated by Chef for <%= node['name'] %> // Changes might be overwritten! EveryWare AG 
command[check_load]=/usr/lib/nagios/plugins/check_load_lxc -w $ARG1$ -c $ARG2$
command[check_disk]=/usr/lib/nagios/plugins/check_disk -w $ARG1$ -c $ARG2$ -p $ARG3$
command[check_quota]=/usr/lib/nagios/plugins/check_disk -e -w $ARG1$ -c $ARG2$ -r $ARG3$ -X nullfs -X devfs 
command[check_swap]=/usr/lib/nagios/plugins/check_swap_lxc -w $ARG1$ -c $ARG2$ 
command[check_mem]=/usr/lib/nagios/plugins/check_mem_lxc -u -w $ARG1$ -c $ARG2$ -C
command[check_mailq]=/usr/lib/nagios/plugins/check_mailq -w $ARG1$ -c $ARG2$ -M $ARG3$
command[check_procs_total]=/usr/lib/nagios/plugins/check_procs 
command[check_procs_var]=/usr/lib/nagios/plugins/check_procs -w $ARG1$ -c $ARG2$ -a $ARG3$
command[check_glusterfs]=/usr/lib/nagios/plugins/check_glusterfs -v $ARG1$ -n $ARG2$ -w $ARG3$ -c $ARG4$
command[check_elasticsearch]=/usr/lib/nagios/plugins/check_http -H localhost -u /_cluster/health -p 9200 -w 2 -c 3 -s green
command[check_mounted_glusterfs]=/usr/lib/nagios/plugins/check_disk -w 20% -c 9% -E -p $ARG1$
command[event_mountall]=/usr/bin/sudo /bin/mount -a && echo OK mount -a
command[check_sysview_proc_svhttpd]=/usr/lib/nagios/plugins/check_procs -w 1: -c 1: --ereg-argument-array="[S]Vhttpd.pl"
command[check_dumpmysql]=/usr/lib/nagios/plugins/check_dumpmysql $ARG1$
